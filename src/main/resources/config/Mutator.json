          "newFieldName":     "timestamp",
          "changeDateConfig": {
            "dateFormatType": "ISO"
          }
        }
      },
      "dataType":   {
        "sizeByte":    "NUMBER",
        "requestTime": "NUMBER"
      }
    }
  },
