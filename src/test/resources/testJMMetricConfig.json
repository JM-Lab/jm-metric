  {
    "configId": "CombinedLogFormat",
    "metricConfigType": "APACHE_ACCESS_LOG",
    "bindDataIds": [],
    "format": "%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-agent}i\""
  },
  {
    "configId": "nginxAccessLogSample",
    "metricConfigType": "NGINX_ACCESS_LOG",
    "bindDataIds": [],
    "format": "$remote_addr - $remote_user [$time_local] \"$request\" $status $body_bytes_sent \"$http_referer\" \"$http_user_agent\"",
    "fieldNameMap": {
      "$remote_addr": "remoteHost",
      "$remote_user": "remoteUser",
      "$time_local": "timestamp",
      "$request": "request",
      "$status": "statusCode",
      "$body_bytes_sent": "sizeByte",
      "$http_referer": "referer",
      "$http_user_agent": "userAgent"
    },
    "fieldConfig": {
      "format": {
        "request": {
          "fields": ["requestMethod", "requestUrl", "requestProtocol"],
          "metricConfigType": "DELIMITER"
        }
      },
      "rawData": true,
      "dateFormat": {
        "timestamp": {
          "dateFormatType": "CUSTOM",
          "format": "dd/MMM/yyyy:HH:mm:ss Z",
          "zoneOffset": "",
          "changeDateConfig": {
            "dateFormatType": "CUSTOM",
            "format": "dd/MMM/yyyy:HH:mm:ss Z",
            "zoneOffset": "+1000"
          }
        }
      },
      "combinedId": ["remoteHost", "requestUrl"],
      "dataType": {
        "statusCode": "WORD",
        "userAgent": "WORD",
        "sizeByte": "NUMBER",
        "requestTime": "NUMBER"
      },
      "unit": {
        "requestTime": "MicroSecond"
      },
      "ignore": ["remoteUser", "remoteLogName"],
      "custom": {
        "customKey": "customValue",
        "customObject": {
          "bool": false
        },
        "customList": ["hello", "world"]
      }
    }
  }
]
