{
  "format": {
    "request": {
      "fields": ["requestMethod", "requestUrl", "requestProtocol"],
      "metricConfigType": "DELIMITER"
    }
  },
  "dateFormat": {
    "timestamp": {
      "dateFormatType": "CUSTOM",
      "format": "dd/MMM/yyyy:HH:mm:ss Z",
      "zoneOffset": "",
      "changeDateFormat": {
        "dateFormatType": "CUSTOM",
        "format": "yyyy-MM-dd'T'HH:mm:ssZ",
        "zoneOffset": "+1000"
      }
    }
  },
  "combinedFields": [
    {
      "targetFields": ["remoteHost", "requestUrl"],
      "combinedFieldName": "",
      "delimiter": "|"
    }
  ],
  "dataType": {
    "statusCode": "WORD",
    "userAgent": "WORD",
    "sizeByte": "NUMBER",
    "requestTime": "NUMBER"
  },
  "unit": {
    "requestTime": "MicroSecond"
  },
  "ignore": ["remoteUser", "remoteLogName"],
  "custom": {
    "customKey": "customValue",
    "customObject": {
      "bool": false
    },
    "customList": ["hello", "world"]
  }
}